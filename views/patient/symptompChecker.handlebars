                    <span class="input-group-text cyan lighten-2" id="basic-text1"><i class="fas fa-search text-dark" aria-hidden="true"></i></span>
                  </div>
                    <input class="form-control my-0 py-1" type="text" name="symptom" placeholder="Search" aria-label="Search">
                </div>
              </div>

                <!-- End of SearchBox               -->


                <!-- CheckBox Input for Symptomp start-->
            <div class="row justify-content-center">
              <div class="containerCB col-12">
                <ul class="ks-cboxtags">
                <li><input type="checkbox" id="checkboxOne" name="fever" value="fever"><label for="checkboxOne">Fever</label></li>
                <li><input type="checkbox" id="checkboxTwo" name="vomiting" value="vomiting"><label for="checkboxTwo">Vomiting</label></li>
                <li><input type="checkbox" id="checkboxThree" name="nausea" value="nausea"><label for="checkboxThree">Nausea</label></li>
                <li><input type="checkbox" id="checkboxFour" name="loss_of_appetite" value="loss of appetite"><label for="checkboxFour">Loss of Appetite</label></li>
                <li><input type="checkbox" id="checkboxFive" name="weight_loss" value="weight loss"><label for="checkboxFive">Weight Loss</label></li>
                <li><input type="checkbox" id="checkboxSix" name="abdominal_pain" value="abdominal pain"><label for="checkboxSix">Abdominal Pain</label></li>
                <li><input type="checkbox" id="checkboxSeven" name="fatigue" value="fatigue"><label for="checkboxSeven">Fatigue</label></li>
                <li><input type="checkbox" id="checkboxEight" name="shortness_of_breath" value="shortness of breath"><label for="checkboxEight">Shortness of Breath</label></li>
                <li><input type="checkbox" id="checkboxNine" name="sweating" value="sweating"><label for="checkboxNine">Sweating</label></li>
                <li><input type="checkbox" id="checkboxTen" name="cough" value="cough"><label for="checkboxTen">Cough</label></li>
                <li class="ks-selected"><input type="checkbox" id="checkboxEleven" name="wheezing" value="wheezing"><label for="checkboxEleven">Wheezing</label></li>
                <li><input type="checkbox" id="checkboxTwelve" name="headache" value="headache"><label for="checkboxTwelve">Headache</label></li>
                <li><input type="checkbox" id="checkboxThirteen" name="chest_pain" value="chest pain"><label for="checkboxThirteen">Chest Pain</label></li>
                <li><input type="checkbox" id="checkboxFourteen" name="chills" value="chills"><label for="checkboxFourteen">Chills</label></li>
                <li><input type="checkbox" id="checkboxFifteen" name="breathing_difficulties" value="breathing difficulties"><label for="checkboxFifteen">Breathing Difficulties</label></li>
  
                </ul>

              </div>
             </div>
             <!-- CheckBox for Symptomps Checker End -->
              
              <div classs="row d-flex justify-content-center">
                <div class="container col-12 text-center">
                <button type="submit" class="btn btn-primary">Submit</button>
